{
  "family": "my-task",
  "networkMode": "awsvpc",
  "cpu": "1 vCPU",
  "memory": "3 GB",
  "volumes": [],
  "executionRoleArn": "arn:aws:iam::__AWS_ACCONT_COD__:role/ecsTaskExecutionRole",

  "containerDefinitions": [
    {
      "name": "teste-devops",
      "image": "730335647486.dkr.ecr.us-east-1.amazonaws.com/my-repo:6d91026eb1c671d873434f8f8bf5127bdb8b951b",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"]
}